        <CanGrow>true</CanGrow>
        <KeepTogether>true</KeepTogether>
        <Paragraphs>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value>=IIf(Not IsNothing(Fields!SP.Value) AND Fields!SP.Value &lt;&gt; "", "Price $" &amp; Fields!SP.Value, IIf(Not IsNothing(Fields!Price.Value) AND Fields!Price.Value &lt;&gt; "", "Price $" &amp; Fields!Price.Value, IIf(Not IsNothing(Fields!MSRP.Value) AND Fields!MSRP.Value &lt;&gt; "", "Price $" &amp; Fields!MSRP.Value, "")))</Value>
